# http://editorconfig.org
root = true

[javascript]
editor.defaultFormatter = "esbenp.prettier-vscode"

# [json]
# editor.defaultFormatter = "esbenp.prettier-vscode"

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = true
